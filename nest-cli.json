{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/santa-close-app/src",
  "monorepo": true,
  "root": "apps/santa-close-app",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/santa-close-app/tsconfig.app.json"
  },
  "projects": {
    "santa-close-app": {
      "type": "application",
      "root": "apps/santa-close-app",
      "entryFile": "main",
      "sourceRoot": "apps/santa-close-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/santa-close-app/tsconfig.app.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "web-common": {
      "type": "library",
      "root": "libs/web-common",
      "entryFile": "index",
      "sourceRoot": "libs/web-common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/web-common/tsconfig.lib.json"
      }
    }
  }
}